{
    "presets": [
        "es2015",
        "react",
        "stage-0"
    ],
    "plugins": [
        "add-module-exports",
        "transform-decorators-legacy",
        "transform-es3-member-expression-literals",
        "transform-es3-property-literals",
         // "antd"//,
        ["import", { "libraryName": "antd", "style": "css" }] // `style: true` 会加载 less 文件
    ]
}
